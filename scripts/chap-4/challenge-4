--- SQL queries

-- 1
SELECT prod_id, prod_name FROM Products
WHERE prod_price = 9.49;


-- 2
SELECT prod_id, prod_name FROM Products
WHERE prod_price > 9;


-- 3
SELECT DISTINCT(order_num) FROM Orders
WHERE order_num > 100; -- Not sure if this is the exact filter logic

-- 4
SELECT prod_name, prod_price FROM Products
WHERE prod_price BETWEEN 3 AND 6
ORDER BY prod_price;